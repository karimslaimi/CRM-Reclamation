                                    @claimsIdentity.FindFirst(x => x.Type == "name").Value
                                </div>
                                <div class="text-xs text-theme-41 dark:text-gray-600"> @claimsIdentity.FindFirst(x => x.Type == System.Security.Claims.ClaimTypes.Role).Value</div>
                            </div>
                            <div class="p-2">
                                <a href="@Url.Action("profile","Client")" class="flex items-center block p-2 transition duration-300 ease-in-out hover:bg-theme-1 dark:hover:bg-dark-3 rounded-md"> <i data-feather="user" class="w-4 h-4 mr-2"></i> Profile </a>
                            </div>
                            <div class="p-2 border-t border-theme-40 dark:border-dark-3">
                                <a href="@Url.Action("logout","Users")" class="flex items-center block p-2 transition duration-300 ease-in-out hover:bg-theme-1 dark:hover:bg-dark-3 rounded-md"> <i data-feather="toggle-right" class="w-4 h-4 mr-2"></i> Logout </a>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- END: Account Menu -->
            </div>
            <!-- END: Top Bar -->

            @RenderBody()


        </div>
        <!-- END: Content -->



